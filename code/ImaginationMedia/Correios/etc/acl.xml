<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="ImaginationMedia_Correios::imaginationmedia_general" title="Imagination Media"
                          sortOrder="0">
                    <resource id="ImaginationMedia_Correios::correios_menutop" title="Correios" sortOrder="0">
                        <resource id="ImaginationMedia_Correios::correios_menuoption1" title="Shipping Track"
                                  sortOrder="1"/>
                        <resource id="ImaginationMedia_Correios::correios_menuoption2" title="Shipping Method Configuration"
                                  sortOrder="2"/>
                        <resource id="ImaginationMedia_Correios::correios_menuoption3" title="Post Methods Configuration"
                                  sortOrder="3"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>